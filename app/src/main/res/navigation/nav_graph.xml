<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_graph"
    app:startDestination="@id/fragmentA">

    <include
        app:graph="@navigation/logout_nav_graph" />

    <include
        app:graph="@navigation/storage_nav_graph" />

    <fragment
        android:id="@+id/fragmentA"
        android:name="br.com.petlove.androidteamprogramming.navigation.FragmentA"
        android:label="Fragment A"
        tools:layout="@layout/fragment_a">
        <action
            android:id="@+id/action_fragmentA_to_fragmentB"
            app:destination="@id/fragmentB" />
        <action
            android:id="@+id/action_fragmentA_to_logout_graph"
            app:destination="@id/logout_graph" />
    </fragment>

    <fragment
        android:id="@+id/fragmentB"
        android:name="br.com.petlove.androidteamprogramming.navigation.FragmentB"
        android:label="Fragment B"
        tools:layout="@layout/fragment_b" />

</navigation>